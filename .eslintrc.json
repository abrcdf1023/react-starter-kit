{
  "env": {
    "browser": true
  },
	"extends": "airbnb",
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module"
	},
	"rules": {
		"generator-star-spacing": "off",
		"semi": 0,
		"import/no-unresolved": 0,
		"no-param-reassign": 0,
		"import/extensions": 0,
		"react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }]
	}
}